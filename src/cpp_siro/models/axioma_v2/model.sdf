   <link name="base_scan">
    <pose> 0 0 0.15 0 0 0</pose>
        <inertial> 
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0.000</ixy>
            <ixz>0.000</ixz>
            <iyy>0.001</iyy>
            <iyz>0.000</iyz>
            <izz>0.001</izz>
          </inertia>
          <mass>0.125</mass>
        </inertial>
        <collision name="lidar_sensor_collision"> 
          <geometry>
            <cylinder>
              <radius>0.035</radius>
              <length>0.030</length>
            </cylinder>
          </geometry>
        </collision>
        <visual name="lidar_sensor_visual"> 
          <geometry>
            <cylinder>
              <radius>0.035</radius>
              <length>0.030</length>
            </cylinder>
          </geometry>
        </visual>
        <sensor name="hls_lfcd_lds" type="ray">
          <pose>0 0 0 0 0 0</pose>
          <always_on>true</always_on>
          <visualize>true</visualize> 
          <update_rate>20</update_rate>
          <ray>
            <scan>
              <horizontal>
                <samples>360</samples>
                <resolution>2.000000</resolution>
                <min_angle>0.000000</min_angle>
                <max_angle>6.280000</max_angle>
              </horizontal>
            </scan>
            <range>
              <min>0.120000</min>
              <max>3.5</max>
              <resolution>0.02000</resolution>
            </range>
            <!--noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.01</stddev>
            </noise-->
          </ray>
          <plugin name="laserscan" filename="libgazebo_ros_ray_sensor.so">
            <ros>
              <!--namespace>/axioma</namespace--> 
                <remapping>~/out:=scan</remapping>
            </ros>
            
            <output_type>sensor_msgs/LaserScan</output_type>
            <frame_name>base_link</frame_name> <!--TODO: cambiar al frame que coinicda en el urdf-->
          </plugin>  
        </sensor>
        <!--sensor name="intel_realsense_r200_depth" type="depth">
